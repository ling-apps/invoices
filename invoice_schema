invoices: [
  {
    invoiceNumber :number autoinc,
    id :id,
    name :text,
    author :text,
    createdAt :date,
    updatedAt :date,
    dueDate :date,
    date :date,
    currency :currency,
    items: [
      {
        description :text,
        quantity :number,
        rate: number
      }
    ]
    vat :oneOf(false, flat, percentage),
    taxes :number,
    notes :text,
    terms :text
  }
]
